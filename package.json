{
  "name": "ai-finder-saas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "build:production": "npm run type-check && npm run lint && next build",
    "start:production": "NODE_ENV=production next start",
    "clean": "rm -rf .next out",
    "test:db": "curl http://localhost:3000/api/auth/debug",
    "fix:collections": "curl -X POST http://localhost:3000/api/auth/fix-collections",
    "reset:auth": "curl -X POST http://localhost:3000/api/auth/reset-auth",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:ci": "npm run lint && npm run type-check && npm run test:coverage && npm run test:e2e",
    "test:production": "NODE_ENV=production npm run test:ci",
    "test:performance": "node scripts/simple-performance-test.js"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.9.1",
    "@clerk/nextjs": "^6.22.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/cheerio": "^0.22.35",
    "@types/nodemailer": "^6.4.17",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.0",
    "dotenv": "^17.2.0",
    "framer-motion": "^12.19.2",
    "imagekit": "^6.0.0",
    "imagekitio-next": "^1.0.1",
    "lucide-react": "^0.518.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.0",
    "next": "15.3.4",
    "nodemailer": "^7.0.5",
    "rate-limiter-flexible": "^7.1.1",
    "razorpay": "^2.9.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "styled-components": "^6.1.19",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.42.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.15.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.2.3",
    "playwright": "^1.42.1",
    "supertest": "^7.0.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
